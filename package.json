{
  "name": "kitsu-inactivity-pruner",
  "version": "2.0.0",
  "description": "Prune inactive accounts",
  "license": "MIT",
  "author": "James Harris <wopian@wopian.me>",
  "scripts": {
    "start": "babel-node src",
    "test": " "
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wopian/kitsu-inactivity-pruner.git"
  },
  "bugs": {
    "url": "https://github.com/wopian/kitsu-inactivity-pruner/issues"
  },
  "homepage": "https://github.com/wopian/kitsu-inactivity-pruner#readme",
  "dependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-0": "~6.24.1",
    "client-oauth2": "~4.1.0",
    "input": "~1.0.1",
    "kitsu": "~3.2.0-rc.2",
    "moment": "~2.19.1",
    "ora": "~1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "~8.0.2",
    "eslint": "~4.11.0",
    "generate-changelog": "~1.6.0",
    "standard": "~10.0.3"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "assignees": [
      "wopian"
    ],
    "pinVersions": false,
    "automerge": false,
    "commitMessage": "{{semanticPrefix}}update {{depName}} to {{newVersion}}",
    "labels": [
      "dependencies"
    ],
    "lockFileMaintenance": {
      "enabled": false
    },
    "semanticCommits": true,
    "separateMajorReleases": false,
    "unpublishSafe": true,
    "schedule": [
      "before 5am on monday"
    ],
    "prCreation": "not-pending",
    "prTitle": "{{semanticPrefix}}{{#if isPin}}pin{{else}}update{{/if}} {{depName}} to {{#if isRange}}{{newVersion}}{{else}}{{#if isMajor}}{{newVersionMajor}}.x{{else}}{{newVersion}}{{/if}}{{/if}}",
    "packageRules": [
      {
        "packagePatterns": [
          "^babel"
        ],
        "groupName": "babel"
      }
    ]
  }
}
